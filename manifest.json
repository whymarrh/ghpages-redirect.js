{
	"permissions": ["webRequest", "webRequestBlocking", "http://*.github.io/*"],
	"background": {
		"scripts": ["ghpages-redirect.js"]
	},
	"name": "HTTPS GitHub Pages",
	"version": "1.0.0",
	"manifest_version": 2,
	"description": "Redirects all requests for GitHub Pages to use HTTPS.",
	"homepage_url": "https://github.com/whymarrh/ghpages-redirect.js"
}
